{
  "name": "@ohtomi/react-whiteboard",
  "version": "0.1.2",
  "description": "A whiteboard React component using SVG",
  "keywords": [
    "react",
    "svg",
    "whiteboard"
  ],
  "homepage": "https://github.com/ohtomi/react-whiteboard",
  "bugs": {
    "url": "https://github.com/ohtomi/react-whiteboard/issues"
  },
  "license": "MIT",
  "author": "Kenichi Ohtomi",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohtomi/react-whiteboard.git"
  },
  "scripts": {
    "build": "run-s compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "compile": "cross-env webpack --progress --profile",
    "cover": "nyc --reporter=html --reporter=text npm run test",
    "start": "run-s storybook",
    "storybook": "start-storybook -p 9080",
    "test": "jest --notify --notifyMode failure",
    "watch": "run-p watch:*",
    "watch:src": "chokidar src/**/*.ts* -i src/**/*.test.ts* -c \"npm run compile\"",
    "watch:test": "jest --watch --onlyChanged --notify --notifyMode failure"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/react": "^5.1.8",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/storybook__react": "^4.0.2",
    "chokidar-cli": "^2.0.0",
    "cross-env": "^6.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "rimraf": "^3.0.0",
    "tap-notify": "^1.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.4"
  },
  "peerDependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  }
}
